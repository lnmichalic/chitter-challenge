<form action="posts/new" method="get">
  <input type="submit"  value="Create a peep">
</form>

<h1> Peeps </h1>

<br/><br/>
<ul id='peeps'>
  <% @peeps.sort_by { |peep| peep.timestamp }.reverse.each do |peep| %>
  <li>
    Message: <%= peep.message %>
    <%= peep.timestamp.strftime("posted at %I:%M%p on %m/%d/%Y") %>
  </li>
  <% end %>
</ul>
